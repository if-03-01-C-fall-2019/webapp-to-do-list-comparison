  85% {
    clip: rect(48px, 9999px, 82px, 0);
  }
  90% {
    clip: rect(52px, 9999px, 11px, 0);
  }
  95% {
    clip: rect(19px, 9999px, 30px, 0);
  }
  100% {
    clip: rect(68px, 9999px, 94px, 0);
  }
}
.glitch:before {
  content: attr(data-text);
  position: absolute;
  left: -2px;
  text-shadow: 1px 0 blue;
  top: 0;
  color: black;
  background: white;
  overflow: hidden;
  clip: rect(0, 900px, 0, 0);
  animation: noise-anim-2 3s infinite linear alternate-reverse;
}


</style>
