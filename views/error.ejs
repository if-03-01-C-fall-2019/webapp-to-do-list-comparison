<!doctype html>
<html lang="en">

<head>
    <title>404 Error</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet">
</head>
<style>
  @import "compass/css3";

 .glitch {
	 color: white;
	 font-size: 100px;
	 position: relative;
	 width: 400px;
	 margin: 0 auto;
}
.glitch {
  color: black;
  font-size: 100px;
  position: relative;
  width: 400px;
  margin: 0 auto;
}
@keyframes noise-anim {
  0% {
    clip: rect(50px, 9999px, 79px, 0);
  }
  5% {
    clip: rect(89px, 9999px, 45px, 0);
  }
  10% {
    clip: rect(54px, 9999px, 96px, 0);
  }
  15% {
    clip: rect(25px, 9999px, 76px, 0);
  }
  20% {
    clip: rect(5px, 9999px, 4px, 0);
  }
  25% {
    clip: rect(21px, 9999px, 6px, 0);
  }
  30% {
    clip: rect(23px, 9999px, 74px, 0);
  }
  35% {
    clip: rect(61px, 9999px, 87px, 0);
  }
  40% {
    clip: rect(5px, 9999px, 33px, 0);
  }
  45% {
    clip: rect(32px, 9999px, 94px, 0);
  }
  50% {
    clip: rect(91px, 9999px, 31px, 0);
  }
  55% {
    clip: rect(71px, 9999px, 71px, 0);
  }
  60% {
    clip: rect(13px, 9999px, 90px, 0);
  }
  65% {
    clip: rect(42px, 9999px, 60px, 0);
  }
  70% {
    clip: rect(97px, 9999px, 68px, 0);
  }
  75% {
    clip: rect(69px, 9999px, 58px, 0);
  }
  80% {
    clip: rect(72px, 9999px, 18px, 0);
  }
  85% {
    clip: rect(33px, 9999px, 22px, 0);
  }
  90% {
    clip: rect(28px, 9999px, 78px, 0);
  }
  95% {
    clip: rect(99px, 9999px, 20px, 0);
  }
  100% {
    clip: rect(15px, 9999px, 45px, 0);
  }
}
.glitch:after {
  content: attr(data-text);
  position: absolute;
  left: 2px;
  text-shadow: -1px 0 red;
  top: 0;
  color: black;
  background: white;
  overflow: hidden;
  clip: rect(0, 900px, 0, 0);
  animation: noise-anim 2s infinite linear alternate-reverse;
}
@keyframes noise-anim-2 {
  0% {
    clip: rect(88px, 9999px, 8px, 0);
  }
  5% {
    clip: rect(18px, 9999px, 69px, 0);
  }
  10% {
    clip: rect(78px, 9999px, 14px, 0);
  }
  15% {
    clip: rect(47px, 9999px, 57px, 0);
  }
  20% {
    clip: rect(21px, 9999px, 33px, 0);
  }
  25% {
    clip: rect(1px, 9999px, 44px, 0);
  }
  30% {
    clip: rect(99px, 9999px, 61px, 0);
  }
  35% {
    clip: rect(6px, 9999px, 14px, 0);
  }
  40% {
    clip: rect(57px, 9999px, 96px, 0);
  }
  45% {
    clip: rect(64px, 9999px, 95px, 0);
  }
  50% {
    clip: rect(39px, 9999px, 82px, 0);
  }
  55% {
    clip: rect(85px, 9999px, 58px, 0);
  }
  60% {
    clip: rect(64px, 9999px, 12px, 0);
